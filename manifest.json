{
    "manifest_version": 3,
    "name": "SEOlytics",
    "description": "An SEO analysis tool for web pages.",
    "version": "1.0",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "host_permissions": ["<all_urls>"]
  }
  